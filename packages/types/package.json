{
  "name": "@respond-framework/types",
  "version": "0.2.0-from-git.0",
  "description": "Shared types for Rudy",
  "rudy-src-main": "src/index.ts",
  "types": "ts/index.d.ts",
  "repository": "https://github.com/respond-framework/rudy/tree/master/packages/types",
  "contributors": [
    "Daniel Playfair Cal <daniel.playfair.cal@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "ts"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "build": "tsc -b",
    "clean": "rimraf ts *.tsbuildinfo",
    "prettier": "prettier",
    "is-pretty": "prettier --ignore-path=../../config/.prettierignore '**/*' --list-different",
    "prettify": "prettier --ignore-path=../../config/.prettierignore '**/*' --write",
    "lint": "eslint --config ../../.eslintrc.js --ignore-path ../../.eslintignore --ext=ts,tsx,js,jsx ."
  }
}
