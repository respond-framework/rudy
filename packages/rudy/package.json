{
  "name": "@respond-framework/rudy",
  "version": "0.1.1-test.4",
  "description": "think of your app in states not routes (and, yes, while keeping the address bar in sync)",
  "main": "dist/index.js",
  "module": "es/index.js",
  "rudy-src-main": "src/index.js",
  "types": "ts/index.d.ts",
  "files": [
    "dist",
    "es",
    "ts"
  ],
  "scripts": {
    "prepare": "yarn run build",
    "build:cjs": "babel --root-mode upward --source-maps true -x .tsx,.ts,.js,.jsx src -d dist",
    "build:es": "babel --root-mode upward --source-maps true -x .tsx,.ts,.js,.jsx --env-name es src -d es",
    "build:ts": "tsc -b",
    "build": "yarn run build:cjs && yarn run build:es && yarn run build:ts && yarn run flow-copy",
    "flow-copy": "flow-copy-source src dist && flow-copy-source src es",
    "flow-watch": "clear; printf \"\\033[3J\" & npm run flow & fswatch -o ./ | xargs -n1 -I{} sh -c 'clear; printf \"\\033[3J\" && npm run flow'",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "clean": "rimraf dist es ts coverage *.tsbuildinfo",
    "prettier": "prettier",
    "is-pretty": "prettier --ignore-path=../../config/.prettierignore '**/*' --list-different",
    "prettify": "prettier --ignore-path=../../config/.prettierignore '**/*' --write",
    "lint": "eslint --ext=ts,tsx,js,jsx . || true",
    "check-types": "tsc",
    "test": "jest --config ../../jest.config.js --rootDir ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/respond-framework/rudy/tree/master/packages/rudy"
  },
  "author": "James Gillmore <james@faceyspacey.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/respond-framework/rudy/issues"
  },
  "homepage": "https://github.com/respond-framework/rudy",
  "dependencies": {
    "@respond-framework/middleware-change-page-title": "^1.0.1-test.1",
    "@respond-framework/middleware-restore-scroll": "^0.1.0-test.1",
    "@respond-framework/utils": "^0.1.1-test.1",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "resolve-pathname": "^2.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --parser=flow --semi=false --write",
      "eslint --fix",
      "git add"
    ]
  }
}
