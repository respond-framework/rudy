{
  "name": "@respond-framework/middleware-change-page-title",
  "version": "1.0.1-test.1",
  "description": "Rudy middleware to synchronise the browser page title with redux state",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "rudy-src-main": "src/index.js",
  "types": "ts/index.d.ts",
  "repository": "https://github.com/respond-framework/rudy/tree/master/packages/middleware-change-page-title",
  "contributors": [
    "James Gilmore <james@faceyspacey.com>",
    "Daniel Playfair Cal <daniel.playfair.cal@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "es",
    "ts"
  ],
  "scripts": {
    "prepare": "yarn run build",
    "build:cjs": "babel --root-mode upward --source-maps true -x .tsx,.ts,.js,.jsx src -d cjs",
    "build:es": "babel --root-mode upward --source-maps true -x .tsx,.ts,.js,.jsx src --env-name es -d es",
    "build": "yarn run build:cjs && yarn run build:es && yarn run flow-copy",
    "clean": "rimraf cjs && rimraf es",
    "flow-copy": "flow-copy-source src cjs && flow-copy-source src es",
    "prettier": "prettier",
    "is-pretty": "prettier --ignore-path=../../config/.prettierignore '**/*' --list-different",
    "prettify": "prettier --ignore-path=../../config/.prettierignore '**/*' --write",
    "lint": "eslint --config ../../.eslintrc.js --ext=ts,tsx,js,jsx --ignore-path ../../.eslintignore --ext=js,jsx .",
    "test": "jest --config ../../jest.config.js --rootDir ."
  },
  "dependencies": {
    "@respond-framework/utils": "^0.1.1-test.1"
  },
  "peerDependencies": {
    "@respond-framework/rudy": "^0.1.1-test.4"
  }
}
